      "items": [
        {
          "type": "Button",
          "caption": "Datenbanken anzeigen",
          "onClick": "$data = HGDB_GetDatabases($id); if (empty($data)) {echo 'Es ist ein Fehler aufgetreten!';} else {print_r($data);}"
        },
        {
          "type": "Button",
          "caption": "Daten aktualisieren",
          "onClick": "$result = HGDB_UpdateData($id, false); if ($result) {echo 'Die Daten wurden aktualisiert!';} else {echo 'Es ist ein Fehler aufgetreten!';}"
        },
        {
          "type": "Button",
          "caption": "Benachrichtigungen versenden",
          "onClick": "$result = HGDB_Notify($id, false); if ($result) {echo 'Die Benachrichtigungen wurden versendet!';} else {echo 'Es ist ein Fehler aufgetreten!';}"
        },
        {
          "type": "Button",
          "caption": "Statusliste anzeigen",
          "onClick": "HGDB_ShowStateList($id);"
        },
        {
          "type": "Button",
          "caption": "Statusliste zurücksetzen",
          "onClick": "HGDB_ResetStateList($id); echo 'Die Statusliste wurde zurückgesetzt!';"
        }
      ]
    }
